<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="my_testng-suite">
	<listeners>
		<listener class-name="com.QALegendBilling.listeners.TestListener" />
	</listeners>
	<test thread-count="5" name="chromeTest">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.QALegendBilling.testscript.HomeTest">
				<methods>
					<exclude name="tc_004_verifyUserManagementtabs()" />
					<exclude name="tc_005_verifyUserSearchWithValidData()"/>
				</methods>
			</class>
			<class name="com.QALegendBilling.testscript.LoginTest">
				<methods>
					<exclude name="tc_001_verifyLoginUsingValidCredentials()"/>
					<exclude name="tc_003_verifyNavigationToLoginPageClickingSignOut()"/>
				</methods>
			</class>
			<class name="com.QALegendBilling.testscript.ResetTest">
				<methods>
					<exclude name="tc_002_verifyResetPasswordField()"/>
				</methods>
			</class>
			<class name="com.QALegendBilling.testscript.AddTest">
				<methods>
					<exclude name="tc_006_verifyErrorMessageDisplayedWithoutFillingMandatoryFieldinAddUser()"/>
					<exclude name="tc_007_verifyUserLoginWithNewlyAddedUser()"/>
					<include name="tc_008_verifyDeleteUser()"/>
					<exclude name="tc_010_verifyShowEntries()"/>
				</methods>
			</class>
		</classes>
	</test>
	
</suite> 

